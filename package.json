{
  "name": "channel-hunter",
  "version": "0.3.0",
  "description": "Music player built with ReactJS",
  "author": "gridsane <gridsane@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/gridsane/channel-hunter.git"
  },
  "scripts": {
    "postinstall": "npm run build",
    "clean": "rm -rf assets/*.js assets/*.map build",
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "webpack --config webpack.config.prod.js",
    "build-server": "babel -d build src",
    "start": "NODE_ENV=production node build/server.js",
    "nodemon": "nodemon -x 'babel-node' -w './src/server' src/server.js",
    "test": "NODE_ENV=test mocha --compilers=js:babel-core/register --require=babel-polyfill --require=./test/setup --recursive './test/**/*_test.js'",
    "test_wo_db": "npm run test -- --grep @database --invert",
    "test_now": "npm run test -- --watch-extensions json --watch --grep @now",
    "lint": "eslint src test"
  },
  "engines": {
    "node": "4.2.1",
    "npm": "2.14.17"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.1",
    "brfs": "^1.4.3",
    "classnames": "^2.2.5",
    "color": "^0.11.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "mongodb": "^2.1.20",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-shallow-compare": "^15.1.0",
    "react-addons-update": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-player": "^0.7.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-persist": "^3.1.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "superagent": "^2.0.0-alpha.3",
    "superagent-jsonp": "0.0.7",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "css-modules-require-hook": "^4.0.0",
    "eslint": "2.10.x",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "jsdom": "^9.2.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "nodemon": "^1.9.2",
    "react-addons-test-utils": "^15.1.0",
    "react-shallow-testutils": "^2.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.6",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
