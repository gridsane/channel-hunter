  "main": "index.js",
  "scripts": {
    "clean": "rm -rf assets/*.js assets/*.map",
    "start": "if [ -z $PORT ]; then PORT=$npm_package_config_devPort; fi; ./node_modules/.bin/babel-node server.js",
    "nodemon": "if [ -z $PORT ]; then PORT=$npm_package_config_devPort; fi; ./node_modules/.bin/nodemon -x './node_modules/.bin/babel-node' -w './src/server' server.js",
    "build": "npm run clean && ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "test": "./node_modules/.bin/mocha --compilers=js:babel-core/register --require ./test/setup --recursive './test/**/*_test.js'",
    "test_wo_db": "npm run test -- --grep @database --invert",
    "lint": "./node_modules/.bin/eslint src test"
  },
  "config": {
    "devPort": 3000
  },
  "author": "",
  "license": "ISC",
