{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "no-debugger": [1],
    "no-empty": [1],
    "no-extra-semi": [2],
    "semi": [2, "always"],
    "no-undef": [2],
  }
}
